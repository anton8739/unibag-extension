{
  "name": "Unibag",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Say hello to your new Shopping Assistant",
  "content_scripts": [
    {
      "matches": [
        "*://*.lamoda.by/*",
        "*://*.lamoda.ru/*",
        "*://*.ozon.ru/*",
        "*://*.ozon.by/*",
        "*://*.wildberries.by/*",
        "*://*.wildberries.ru/*",
        "*://*.aliexpress.ru/*",
        "*://*.market.yandex.ru/*",
        "*://*.market.yandex.by/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "icons": {
    "16": "/asserts/icons/logo-icon-16x16.png",
    "32": "/asserts/icons/logo-icon-32x32.png",
    "48": "/asserts/icons/logo-icon-48x48.png",
    "128": "/asserts/icons/logo-icon-128x128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/asserts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}